#include <stdio.h>
#include <locale.h>
#include <string.h>

/*Escreva um algoritmo que leia um número N e mostre todos os múltiplos de 3
e 5, ao mesmo tempo, no intervalo fechado de 1 a N.*/


int main(){
  int n,i;
  setlocale(LC_ALL,"Portuguese");
  printf("Digite um número: ");
  scanf("%d",&n);

  for(i=1;i<=n;i++){
    if(i%3==0 && i%5==0){
        printf("%d \n",i);
    }
  }


  return 0;

}
