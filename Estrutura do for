#include <locale.h>
#include <string.h>

/*A conversão de graus Fahrenheit para Celcius é obtida por C=(F32)/1.8. Faça um programa que calcule e escreva uma tabela de graus
Fahrenheit em graus Celsius, cujos graus variem de 50 a 65 de 1 em 1.*/



int main(){
  int i;
  setlocale(LC_ALL,"Portuguese");

  printf("De Fahrenheit para Celcius\n\n");
  for(i=50;i<=65;i++){
    printf("%dF = %d°C\n",i,i-32);
  }


  return 0;

}
